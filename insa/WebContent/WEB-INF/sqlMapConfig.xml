	<!-- 별칭 -->
	<!-- mappinh annotation -->
	
	<typeAliases>
		<typeAlias type="kr.co.nethru.vo.UserVo" alias="UserVo"/>
	</typeAliases>
	
	
	<!-- sql xml 연결 -->
	<mappers>
		<mapper resource="ino/insa/youngwoo/gesipanQuery/WooGesipan.xml" />
	</mappers>
</configuration>
 	

